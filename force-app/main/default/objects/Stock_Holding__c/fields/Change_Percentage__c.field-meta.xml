<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Change_Percentage__c</fullName>
    <description>Percentage change in stock price</description>
    <externalId>false</externalId>
    <formula>IF(ISNULL(Purchase_Price__c) || Purchase_Price__c = 0, 0, (Current_Price__c - Purchase_Price__c) / Purchase_Price__c * 100)</formula>
    <formulaTreatBlanksAs>Zero</formulaTreatBlanksAs>
    <label>Change Percentage</label>
    <precision>18</precision>
    <scale>2</scale>
    <type>Percent</type>
</CustomField>